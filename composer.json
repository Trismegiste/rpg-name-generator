{
    "name": "trismegiste/rpg-name-generator",
    "description": "Library for generating random names (for table-top roleplaying games)",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Trismegiste"
        }
    ],
    "require": {
        "php": ">7.3",
        "symfony/yaml": "^5.3",
        "symfony/finder": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Trismegiste\\NameGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Trismegiste\\NameGenerator\\": "tests/"
        }
    }
}
